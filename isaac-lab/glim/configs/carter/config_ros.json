{
  "glim_ros": {
    "acc_scale": 1.0,
    "base_frame_id": "",
    "enable_global_mapping": true,
    "enable_local_mapping": true,
    "extension_modules": [
      "libmemory_monitor.so",
      "libstandard_viewer.so",
      "librviz_viewer.so"
    ],
    "image_qos": {
      "profile": "sensor_data"
    },
    "image_topic": "/image",
    "imu_frame_id": "imu",
    "imu_qos": {
      "depth": 1000,
      "profile": "sensor_data"
    },
    "imu_time_offset": 0.0,
    "imu_topic": "/os_cloud_node/imu",
    "keep_raw_points": false,
    "lidar_frame_id": "lidar",
    "map_frame_id": "map",
    "odom_frame_id": "odom",
    "points_qos": {
      "profile": "sensor_data"
    },
    "points_time_offset": 0.0,
    "points_topic": "/os_cloud_node/points",
    "publish_imu2lidar": true,
    "tf_time_offset": 1e-06
  }
}
